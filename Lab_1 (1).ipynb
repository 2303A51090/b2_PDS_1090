        "def fact(n, d=0):\n",
        "    space = \"  \" * d\n",
        "    if n == 0 or n == 1:\n",
        "        return 1\n",
        "    else:\n",
        "        r = n * fact(n - 1, d + 1)\n",
        "        return r\n",
        "\n",
        "n = int(input())\n",
        "if 0 <= n <= 10:\n",
        "    r = fact(n)\n",
        "    print(r)\n",
        "else:\n",
        "    print(\"Invalid\")\n"
